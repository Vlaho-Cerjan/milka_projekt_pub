generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model admins {
  id          Int     @id @default(autoincrement())
  first_name  String? @db.VarChar(255)
  last_name   String? @db.VarChar(255)
  title       String? @db.VarChar(255)
  admin_title String? @db.VarChar(255)
  admin_type  String? @db.VarChar(255)
  create_date String? @db.VarChar(255)
  update_date String? @db.VarChar(255)
  delete_date String? @db.VarChar(255)
  img_src     String? @db.MediumText
  alt         String? @db.MediumText
}

model blog {
  id                Int     @id @default(autoincrement())
  title             String  @db.VarChar(255)
  content           String  @db.MediumText
  img_src           String? @db.MediumText
  categories        Int?
  tags              Int?
  slug              String? @db.VarChar(255)
  create_date       String? @db.VarChar(255)
  update_date       String? @db.VarChar(255)
  delete_date       String? @db.VarChar(255)
  alt               String? @db.MediumText
  short_description String? @db.MediumText
}

model categories {
  id          Int     @id @default(autoincrement())
  name        String  @db.VarChar(255)
  description String? @db.MediumText
  create_date String? @db.VarChar(255)
  update_date String? @db.VarChar(255)
  delete_date String? @db.VarChar(255)
}

model company_info {
  id            Int     @id @default(autoincrement())
  name          String  @db.VarChar(255)
  title         String? @db.VarChar(255)
  email         String? @db.VarChar(255)
  url           String? @db.MediumText
  working_hours String  @db.VarChar(255)
  address       String? @db.MediumText
  address_short String  @db.VarChar(255)
  address_url   String? @db.MediumText
  phone         String? @db.VarChar(255)
  coords        String  @db.Text
}

model doctors {
  id              Int     @id @default(autoincrement())
  first_name      String  @db.VarChar(255)
  aditional_names String? @db.VarChar(255)
  last_name       String? @db.VarChar(255)
  title           String? @db.VarChar(255)
  bio             String? @db.MediumText
  email           String? @db.VarChar(255)
  phone           String? @db.VarChar(255)
  create_date     String? @db.VarChar(255)
  update_date     String? @db.VarChar(255)
  delete_date     String? @db.VarChar(255)
  img_src         String? @db.MediumText
  slug            String? @db.VarChar(255)
  alt             String? @db.MediumText
}

model employes {
  id              Int     @id @default(autoincrement())
  first_name      String  @db.VarChar(255)
  aditional_names String? @db.VarChar(255)
  last_name       String? @db.VarChar(255)
  title           String? @db.VarChar(255)
  bio             String? @db.MediumText
  email           String? @db.VarChar(255)
  phone           String? @db.VarChar(255)
  employe_title   String? @db.VarChar(255)
  create_date     String? @db.VarChar(255)
  update_date     String? @db.VarChar(255)
  delete_date     String? @db.VarChar(255)
  img_src         String? @db.MediumText
  slug            String? @db.VarChar(255)
  alt             String? @db.MediumText
}

model faq {
  id          Int     @id @default(autoincrement())
  title       String  @db.MediumText
  content     String  @db.MediumText
  faq_order   Int
  create_date String? @db.VarChar(255)
  update_date String? @db.VarChar(255)
  delete_date String? @db.VarChar(255)
}

model services {
  id          Int     @id @default(autoincrement())
  name        String  @db.VarChar(255)
  description String? @db.MediumText
  doctors_id  String? @db.VarChar(255)
  create_date String? @db.VarChar(255)
  update_date String? @db.VarChar(255)
  delete_date String? @db.VarChar(255)
  img_src     String? @db.MediumText
  slug        String? @db.VarChar(255)
  alt         String? @db.MediumText
}

model services_list {
  id             Int     @id @default(autoincrement())
  name           String  @db.VarChar(255)
  value          Float?
  discount       Float?
  usluga_id      Int?
  pod_usluga_id  Int?
  description    String? @db.MediumText
  value_range    String? @db.VarChar(255)
  services_order Int?
  highlighted    Int     @default(0) @db.TinyInt
  create_date    String? @db.VarChar(255)
  update_date    String? @db.VarChar(255)
  delete_date    String? @db.VarChar(255)
}

model subservices {
  id          Int     @id @default(autoincrement())
  name        String  @db.VarChar(255)
  usluga_id   Int
  description String? @db.MediumText
  doctors_id  String? @db.VarChar(255)
  create_date String? @db.VarChar(255)
  update_date String? @db.VarChar(255)
  delete_date String? @db.VarChar(255)
  img_src     String? @db.MediumText
  slug        String? @db.VarChar(255)
  alt         String? @db.MediumText
}

model tags {
  id          Int     @id
  NAME        String  @db.VarChar(255)
  create_date String? @db.VarChar(255)
  update_date String? @db.VarChar(255)
  delete_date String? @db.VarChar(255)
}

model page_info {
  id               Int     @id @default(autoincrement())
  title            String? @db.VarChar(255)
  page_title       String  @db.Text
  page_description String  @db.Text
  page_slug        String  @db.Text
  image            String  @db.Text
  openGraphType    String  @db.Text
  createdAt        String  @db.VarChar(255)
  updatedAt        String? @db.VarChar(255)
}

model socials {
  id         Int     @id @default(autoincrement())
  name       String  @db.VarChar(255)
  href       String  @db.Text
  type       String  @db.Text
  created_at String  @default(dbgenerated("(current_timestamp())")) @db.VarChar(255)
  updated_at String? @db.VarChar(255)
  deleted_at String? @db.VarChar(255)
}

model navigation {
  id         Int     @id @default(autoincrement())
  name       String  @db.VarChar(255)
  href       String  @db.VarChar(255)
  type       String  @db.VarChar(255)
  parent_id  Int?
  nav_order  Int
  created_at String? @db.VarChar(255)
  updated_at String? @db.VarChar(255)
  deleted_at String? @db.VarChar(255)
  active     Int     @default(1) @db.TinyInt
}

model services_price_list {
  id              Int     @id @default(autoincrement())
  service_list_id Int
  title           String  @db.VarChar(255)
  description     String? @db.Text
  value           Float
  discount        Float?
  created_at      String  @db.VarChar(255)
  updated_at      String? @db.VarChar(255)
  deleted_at      String? @db.VarChar(255)
  active          Int     @default(1) @db.TinyInt
}
